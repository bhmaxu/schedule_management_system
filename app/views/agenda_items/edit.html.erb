<% provide(:title, "日程编辑") %>
<%= form_for@agenda_item do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.text_area :content, placeholder: "创建新的日程..." %>
  </div>
  <%= f.label "日期" %>
  <%= f.datetime_select :time , :class=>"datetimepicker1"%>
  <%= f.submit "确认修改", class: "btn btn-primary" %>
<% end %>